import csv

with open('ChilliwackTerrainCodes.csv', newline='') as csvfile:
    reader = csv.reader(csvfile)
    next(reader) # skip header row
    result = []
    for row in reader:
        if any(c.isupper() for c in row[1]):
            consecutive_uppercase_count = 0
            for c in row[1]:
                if c.isupper():
                    consecutive_uppercase_count += 1
                    if consecutive_uppercase_count >= 3:
                        result.append(row[1])
                        break
                else:
                    consecutive_uppercase_count = 0
    print(result)
